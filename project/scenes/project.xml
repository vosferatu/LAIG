<SCENE>

<!-- Initial values -->
<INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="5" />
</INITIALS>


<!-- Illumination and clear colour -->
<ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0" a="1" />
</ILLUMINATION>


<!-- Lights -->
<LIGHTS>
	<LIGHT id="light0">
        <enable value ="1" />
        <position  x="0" y="10" z="-60" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

	<LIGHT id="light1">
        <enable value ="1" />
        <position x="30" y="10" z="-60" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

	<LIGHT id="light2">
        <enable value ="1" />
        <position x="60" y="10" z="-60" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light3">
        <enable value ="1" />
        <position x="90" y="10" z="-60" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
        <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

    <LIGHT id="lua">
        <enable value ="1" />
        <position x="-40" y="100" z="-100" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="0.9" g="0.9" b="0.9" a="1.0" />
        <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>


</LIGHTS>

<!-- Textures -->
<TEXTURES>
	<!-- Tree trunk -->
    <TEXTURE id="trunk">
        <file path="images/trunk-image.jpg" />
        <amplif_factor s="2" t="2" />
    </TEXTURE>

     <!-- Tree leaves -->
    <TEXTURE id="folhas">
        <file path="images/leaves.jpg" />
        <amplif_factor s="3" t="4" />
    </TEXTURE>

     <!-- feup -->
    <TEXTURE id="feup">
        <file path="images/feup.png" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Rocks -->
    <TEXTURE id="rocks">
        <file path="images/rocks.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Glass work -->
    <TEXTURE id="vidral">
        <file path="images/vidral.jpg" />
        <amplif_factor s="15" t="10" />
    </TEXTURE>


    <!-- Dirt -->
    <TEXTURE id="dirt">
        <file path="images/dirt.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Street bench -->
    <TEXTURE id="banco">
        <file path="images/bank.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="grass">
      <file path="images/grass.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="lua">
      <file path="images/moon.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="chao_pedra">
      <file path="images/sidewalk2.jpg" />
      <amplif_factor s="4" t="4" />
    </TEXTURE>

    <TEXTURE id="wave">
      <file path="images/wave.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

</TEXTURES>


<MATERIALS>

	<!-- Tree leaves material -->
	<MATERIAL id="leavesTreeMaterial">
        <shininess value="1" />
        <specular r="0.2" g="0.2" b="0.2" a="1" />
        <diffuse r="0.0" g="0.207" b="0.0" a="1" />
        <ambient r="0.1" g="0.1" b="0.1" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

	<!-- Tree trunk material -->
	<MATERIAL id="trunkTreeMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.6" g="0.298" b="0.0" a="1" />
        <ambient r="0.1" g="0.1" b="0.1" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Lamp post material -->
    <MATERIAL id="lightPostMaterial">
        <shininess value="100" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.447" g="0.416" b="0.416" a="1" />
        <ambient r="0.0" g="0.0" b="0.0" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Light bulb material -->
    <MATERIAL id="lightMaterial">
        <shininess value="100" />
        <specular r="0.5" g="0.5" b="0.5" a="1" />
        <diffuse r="0.5" g="0.5" b="0.5" a="1" />
        <ambient r="0.0" g="0.0" b="0.0" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />
        <ambient r="0.4" g="0.4" b="0.4" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

</MATERIALS>

<!-- The animations -->
<ANIMATIONS>

  <ANIMATION id="linear" speed="2.5" type="linear">
    <controlpoint xx="0" yy="0" zz="0" />
    <controlpoint xx="0" yy="0" zz="15" />
  </ANIMATION>

  <ANIMATION id="circular" speed="2.5" type="circular"
    centerx="0" centery="0" centerz="0" radius="3"
    startang="50" rotang="150"/>

  <ANIMATION id="bezier" speed="5" type="bezier">
    <controlpoint xx="0" yy="0" zz="0" /> <!--P1-->
    <controlpoint xx="-2" yy="3" zz="3" /> <!--P2-->
  ​  <controlpoint xx="4" yy="10" zz="5" /> <!--P3-->
    <controlpoint xx="0" yy="-1" zz="7" /> <!--P4-->
  </ANIMATION>
  ​​
  <ANIMATION id="combo" type="combo">
    <SPANREF id="circular" />
    <SPANREF id="linear" />
  </ANIMATION>

</ANIMATIONS>

<!-- The leaves -->
<LEAVES>
</LEAVES>


<!-- The nodes -->
<NODES>

    <ROOT id="root" />

    <!-- The root node -->
    <NODE id="root">
        <MATERIAL id="defaultMaterial" />
        <TEXTURE id="null" />

        <DESCENDANTS>

            <NODEREF id="luzes" />
            <NODEREF id="arvores" />
            <NODEREF id="relva" />
            <NODEREF id="bancos" />
            <NODEREF id="queijos" />
            <NODEREF id="predios" />
            <NODEREF id="departamentos" />
            <NODEREF id="lua"/>
            <NODEREF id="canteiros" />
            <NODEREF id="flag" />

        </DESCENDANTS>
    </NODE>

    <!-- canteiros -->
    <NODE id="canteiros">
        <MATERIAL id="null" />
        <TEXTURE id="dirt" />

        <DESCENDANTS>
            <NODEREF id="canteiro1" />
            <NODEREF id="canteiro2" />
            <NODEREF id="canteiro3" />
        </DESCENDANTS>

    </NODE>

    <!-- One canteiro -->
    <NODE id="canteiro1">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="70.5" y="0" z="-15" />
      	<SCALE sx="3" sy="3" sz="3"/>


        <DESCENDANTS>
            <NODEREF id="canteiro" />
        </DESCENDANTS>

    </NODE>

    <!-- second canteiro -->
    <NODE id="canteiro2">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="70.5" y="0" z="-35" />
      	<SCALE sx="5" sy="5" sz="5"/>


        <DESCENDANTS>
            <NODEREF id="canteiro" />
        </DESCENDANTS>

    </NODE>

        <!-- 3rd canteiro -->
    <NODE id="canteiro3">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="70.5" y="0" z="-55" />
      	<SCALE sx="2" sy="2" sz="2"/>


        <DESCENDANTS>
            <NODEREF id="canteiro" />
        </DESCENDANTS>


    </NODE>

            <!-- 4TH canteiro -->
    <NODE id="canteiro4">
        <MATERIAL id="null" />
        <TEXTURE id="dirt" />

        <!--<TRANSLATION x="10" y="0" z="10" />-->
      	<SCALE sx="1" sy="1" sz="1"/>


        <DESCENDANTS>
            <NODEREF id="canteiro" />
        </DESCENDANTS>

    </NODE>

	<NODE id="canteiro">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <LEAF id="canto" type="patch" args="30 30" >
          <CPLINE>
            <CPOINT xx="-1.5" yy="0.0" zz="-1.5" ww="1"/>
            <CPOINT xx="-0.5" yy="0.0" zz="-1.5" ww="1"/>
            <CPOINT xx="0.5" yy="0.0" zz="-1.5" ww="1"/>
            <CPOINT xx="1.5" yy="0.0" zz="-1.5" ww="1"/>
          </CPLINE>
          <CPLINE>
            <CPOINT xx="-1.5" yy="0.0" zz="-0.5" ww="1"/>
            <CPOINT xx="-0.5" yy="0.5" zz="-0.5" ww="1"/>
            <CPOINT xx="0.5" yy="0.5" zz="-0.5" ww="1"/>
            <CPOINT xx="1.5" yy="0.0" zz="-0.5" ww="1"/>
          </CPLINE>
          <CPLINE>
            <CPOINT xx="-1.5" yy="0.0" zz="0.5" ww="1"/>
            <CPOINT xx="-0.5" yy="0.5" zz="0.5" ww="1"/>
            <CPOINT xx="0.5" yy="0.5" zz="0.5" ww="1"/>
            <CPOINT xx="1.5" yy="0.0" zz="0.5" ww="1"/>
          </CPLINE>
          <CPLINE>
            <CPOINT xx="-1.5" yy="0.0" zz="1.5" ww="1"/>
            <CPOINT xx="-0.5" yy="0.0" zz="1.5" ww="1"/>
            <CPOINT xx="0.5" yy="0.0" zz="1.5" ww="1"/>
            <CPOINT xx="1.5" yy="0.0" zz="1.5" ww="1"/>
          </CPLINE>
        </LEAF>
      </DESCENDANTS>
    </NODE>


     <!-- flag -->
    <NODE id="flag">
        <MATERIAL id="null" />
        <TEXTURE id="null" />


        <TRANSLATION x="10" y="0" z="10" />
        <SCALE sx="3" sy="3" sz="3"/>

        <DESCENDANTS>
            <NODEREF id="bandeira" />
            <NODEREF id="pole" />
            <NODEREF id="poletop" />
            <NODEREF id="canteiro4" />

        </DESCENDANTS>

    </NODE>


    <NODE id="pole">
        <MATERIAL id="lightPostMaterial" />
        <TEXTURE id="null" />

        <SCALE sx="0.1" sy="1" sz="0.1" />
        <ROTATION axis="x" angle="-90" />

        <DESCENDANTS>
					<LEAF id="cylinder2" type="cylinder" args="12 1 1 20 20 1 1" />
        </DESCENDANTS>

    </NODE>

        <!-- pole top -->
     <NODE id="poletop">
        <MATERIAL id="trunkTreeMaterial" />
        <TEXTURE id="null" />

		<TRANSLATION x="0" y="12" z="0" />
        <SCALE sx="0.1" sy="0.1" sz="0.1" />

        <DESCENDANTS>
			<LEAF type="sphere" args="1 32 32"/>
        </DESCENDANTS>

    </NODE>

    <NODE id="bandeira">
      <MATERIAL id="null" />
      <TEXTURE id="feup" />

 	  <TRANSLATION x="0" y="5" z="0" />
      <SCALE sx="5" sy="3" sz="5"/>

      <DESCENDANTS>
        <LEAF id="bandeiraTras" type="patch" args="30 30" >
          <CPLINE>
            <CPOINT xx="0" yy="1" zz="0" ww="1"/>
