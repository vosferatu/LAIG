          </CPLINE>
          <CPLINE>
            <CPOINT xx="0.25" yy="1.0" zz="0.25" ww="1"/>
            <CPOINT xx="0.25" yy="0.0" zz="0.25" ww="1"/>
          </CPLINE>
          <CPLINE>
            <CPOINT xx="0.5" yy="1.0" zz="0" ww="1"/>
            <CPOINT xx="0.5" yy="0.0" zz="0" ww="1"/>
          </CPLINE>
          <CPLINE>
            <CPOINT xx="0.75" yy="1.0" zz="-0.25" ww="1"/>
