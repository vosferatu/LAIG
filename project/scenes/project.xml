		<ROTATION angle="-90" axis="x"/>
        <DESCENDANTS>
        	<LEAF type="rectangle" args="0 10 75 0" />
  		</DESCENDANTS>
    </NODE>

    <NODE id="teto">
		    <MATERIAL id="null" />
		    <TEXTURE id="null" />

		    <SCALE sx="10" sy="1" sz="75" />
