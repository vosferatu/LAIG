<SCENE>

  <!-- Initial values -->
  <INITIALS>
    <frustum near="0.1" far="500"/>                 
    <translation x="-1" y="1" z="0" />               
    <rotation axis="x" angle="0" />                
    <rotation axis="y" angle="0" />                
    <rotation axis="z" angle="0" />                
    <scale sx="0.25" sy="0.25" sz="0.25" />         
    <reference length="5" />                        
  </INITIALS>


  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.3" g="0.3" b="0.3" a="1" />       
    <background r="0" g="0" b="0" a="1" />          
  </ILLUMINATION>


  <!-- Lights -->
  <LIGHTS>    
   <LIGHT id="light0">                                    
    <enable value ="1" />                              
    <position  x="10" y="11" z="15" w="1" />           
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />          
    <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />                                        
    <specular r="0.9" g="0.9" b="0.9" a="1.0" />      
  </LIGHT>

  <LIGHT id="light1">                                    
    <enable value ="1" />                             
    <position x="11" y="12" z="11" w="1" />            
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />       
    <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />                                           
    <specular r="0.9" g="0.9" b="0.9" a="1.0" />  
  </LIGHT>

  <LIGHT id="light2">                                   
    <enable value ="1" />                              
    <position x="14" y="11" z="14" w="1" />                
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />      
    <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />                            
    <specular r="0.9" g="0.9" b="0.9" a="1.0" />     
  </LIGHT>

  <LIGHT id="light3">                                   
    <enable value ="1" />                          
    <position x="17" y="7.3" z="5.8"  w="0" />                 
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />             
    <diffuse r="0.9" g="0.9" b="0.9" a="1.0" />       
    <specular r="0.9" g="0.9" b="0.9" a="1.0" />      
  </LIGHT>
  
</LIGHTS>

<!-- Textures -->
<TEXTURES>

  <!-- Hide -->
  <TEXTURE id="sofa">
    <file path="images/sofa.jpg" />                
    <amplif_factor s="1" t="1" />   
  </TEXTURE>

  <!-- Metal -->
  <TEXTURE id="metal">
    <file path="images/metal.jpg" />                
    <amplif_factor s="1" t="1" />   
  </TEXTURE>

  <!-- Screen tv -->
  <TEXTURE id="screen">
    <file path="images/screen.jpg" />                
    <amplif_factor s="1" t="1" />   
  </TEXTURE>

  <!-- Wood -->
  <TEXTURE id="wood">
    <file path="images/wood.png" />                
    <amplif_factor s="0.5" t="0.5" />   
  </TEXTURE>

  <!-- Floor -->
  <TEXTURE id="floor">
    <file path="images/parquet.jpg" />                
    <amplif_factor s="0.25" t="0.25" />   
  </TEXTURE>

  <!-- Glass -->
  <TEXTURE id="glass">
    <file path="images/glass.png" />                
    <amplif_factor s="1" t="1" />   
  </TEXTURE>

  <!-- Box tv -->
  <TEXTURE id="boxTexture">
    <file path="images/box.png" />                
    <amplif_factor s="1" t="1" />   
  </TEXTURE>

  <!-- Fabric -->
  <TEXTURE id="fabric">
    <file path="images/fabric.jpg" />                
    <amplif_factor s="0.5" t="0.5" />   
  </TEXTURE>
</TEXTURES>


<MATERIALS>
	<!-- Pyramid material -->
  <MATERIAL id="piramideMaterial">                                  
    <shininess value="1" />
    <specular r="0" g="0" b="0" a="1" />        
    <diffuse r="1.0" g="0.68" b="0.4" a="1" />        
    <ambient r="0.2" g="0.2" b="0.2" a="1" />         
    <emission r="0" g="0.0" b="0.0" a="1" />        
  </MATERIAL>

  <!-- Tree leaves material -->
  <MATERIAL id="leavesTreeMaterial">                                  
    <shininess value="1" />
    <specular r="0.2" g="0.2" b="0.2" a="1" />        
    <diffuse r="0.0" g="0.207" b="0.0" a="1" />       
    <ambient r="0.1" g="0.1" b="0.1" a="1" />         
    <emission r="0.0" g="0.0" b="0.0" a="1" />        
  </MATERIAL>

  <!-- Tree trunk material -->
  <MATERIAL id="trunkTreeMaterial">                                  
    <shininess value="1" />
    <specular r="0.0" g="0.0" b="0.0" a="1" />        
    <diffuse r="0.6" g="0.298" b="0.0" a="1" />      
    <ambient r="0.1" g="0.1" b="0.1" a="1" />         
    <emission r="0.0" g="0.0" b="0.0" a="1" />       
  </MATERIAL>

  <!-- Lamp post material -->
  <MATERIAL id="lightPostMaterial">                                  
    <shininess value="100" />
    <specular r="0.0" g="0.0" b="0.0" a="1" />        
    <diffuse r="0.447" g="0.416" b="0.416" a="1" />       
    <ambient r="0.0" g="0.0" b="0.0" a="1" />         
    <emission r="0.0" g="0.0" b="0.0" a="1" />        
  </MATERIAL>

  <!-- Light bulb material -->
  <MATERIAL id="lightMaterial">                                  
    <shininess value="100" />
    <specular r="0.5" g="0.5" b="0.5" a="0.5" />       
    <diffuse r="0.5" g="0.5" b="0.5" a="0.5" />       
    <ambient r="0.0" g="0.0" b="0.0" a="1" />        
    <emission r="0.5" g="0.5" b="0.5" a="1" />        
  </MATERIAL>

  <!-- A default material -->
  <MATERIAL id="defaultMaterial">                                  
    <shininess value="1" />
    <specular r="0.0" g="0.0" b="0.0" a="1" />        
    <diffuse r="0.4" g="0.4" b="0.4" a="1" />       
    <ambient r="0.4" g="0.4" b="0.4" a="1" />         
    <emission r="0.0" g="0.0" b="0.0" a="1" />        
  </MATERIAL>

  <!-- Wall Material -->
  <MATERIAL id="wallMaterial">                                  
    <shininess value="0.5" />
    <specular r="0.0" g="0.0" b="0.0" a="1" />        
    <diffuse r="0.66" g="0.66" b="0.66" a="1" />       
    <ambient r="0.66" g="0.66" b="0.66" a="1" />         
    <emission r="0.0" g="0.0" b="0.0" a="1" />        
  </MATERIAL>

  <!-- Black Material -->
  <MATERIAL id="blackMaterial">                                  
    <shininess value="0.5" />
    <specular r="0.1" g="0.1" b="0.1" a="1" />        
    <diffuse r="0" g="0" b="0" a="1" />       
    <ambient r="0.1" g="0.1" b="0.1" a="1" />         
    <emission r="0.0" g="0.0" b="0.0" a="1" />        
  </MATERIAL>

  <!-- TV Material -->
  <MATERIAL id="tvMaterial">                                  
    <shininess value="0.5" />
    <specular r="0.1" g="0.1" b="0.1" a="1" />        
    <diffuse r="0" g="0" b="0" a="1" />       
    <ambient r="0.1" g="0.1" b="0.1" a="1" />         
    <emission r="0.7" g="0.7" b="0.7" a="1" />        
  </MATERIAL>

  <!-- Light bulb material -->
  <MATERIAL id="glassMaterial">                                  
    <shininess value="200" />
    <specular r="0.2" g="0.3" b="0.5" a="0.2" />       
    <diffuse r="0.2" g="0.3" b="0.5" a="0.2" />       
    <ambient r="0.1" g="0.1" b="0.5" a="0.2" />        
    <emission r="0.0" g="0.0" b="0.0" a="1" />        
  </MATERIAL>

</MATERIALS>


